    "noImplicitAny": false,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "target": "es5",
    "removeComments": true,
    "lib": [
      "ES2021"
    ],
  },
  "include": [
    "./src/**/*"
  ]
}
